        {   "description": "s = s", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "s" },		    
            "to": [ { "key_code": "s" } ] },
        {   "description": "d = previous tab", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "d" },		    

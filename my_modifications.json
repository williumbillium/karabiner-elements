        {   "description": "' = backspace", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "quote", "modifiers": { "optional": [ "any" ] } },	    
            "to": [ { "key_code": "delete_forward" } ] },
        {   "description": "n = prev desktop", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],

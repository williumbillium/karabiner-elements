  "rules": [
    { "description": "Modifiers", "manipulators": [
        {   "description": "capslock click = esc | capslock hold = nav", "type": "basic",
            "from": { "key_code": "caps_lock", "modifiers": { "optional": [ "any" ] } },
            "to_if_alone": [ { "key_code": "escape" } ],
            "to": [ { "set_variable": { "name": "layer_name", "value": "nav" } } ],
            "to_after_key_up": [ { "set_variable": { "name": "layer_name", "value": "default" } } ] },
        {   "description": "quote click = doublequote | quote hold = sym", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "default" } ],
            "from": { "key_code": "quote", "modifiers": { "optional": [ "any" ] } },
            "to_if_alone": [ { "key_code": "quote" } ],
            "to": [ { "set_variable": { "name": "layer_name", "value": "sym" } } ],
            "to_after_key_up": [ { "set_variable": { "name": "layer_name", "value": "default" } } ] },
        {   "description": "application = nav", "type": "basic",
            "from": { "key_code": "application", "modifiers": { "optional": [ "any" ] } },
            "to": [ { "set_variable": { "name": "layer_name", "value": "nav" } } ] ,
            "to_after_key_up": [ { "set_variable": { "name": "layer_name", "value": "default" } } ] }
    ]}, 
    { "description": "Nav layer", "manipulators": [
        {   "description": "y = CMD + left", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "y", "modifiers": { "optional": [ "any" ] } },		    
            "to": [ { "key_code": "left_arrow", "modifiers": ["left_command"] } ] },
        {   "description": "u = CMD + down", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "u", "modifiers": { "optional": [ "any" ] } },		    
            "to": [ { "key_code": "down_arrow", "modifiers": ["left_command"] } ] },
        {   "description": "i = CMD + up", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],

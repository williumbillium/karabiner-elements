        {   "description": "right_shift = right_shift", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "right_shift", "modifiers": { "optional": [ "any" ] } },	    
            "to": [ { "key_code": "right_shift" } ] },
        {   "description": "tab = tab", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "tab", "modifiers": { "optional": [ "any" ] } },	    
            "to": [ { "key_code": "tab" } ] },

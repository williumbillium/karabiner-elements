            "from": { "key_code": "w" },
            "to": [ { "key_code": "w" } ] },
        {   "description": "e = back", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "e" },		    

        {   "description": "k = up_arrow", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "k", "modifiers": { "optional": [ "any" ] } },	    
            "to": [ { "key_code": "up_arrow" } ] },
        {   "description": "l = right_arrow", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "l", "modifiers": { "optional": [ "any" ] } },	    
            "to": [ { "key_code": "right_arrow" } ] },

        {   "description": "disable all keys", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "any": "key_code" },		    
            "to": [ { "key_code": "vk_none"  } ] }
    ] },

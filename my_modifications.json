        {   "description": "r = forward", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "r" },
            "to": [ { "pointing_button": "button5" } ] },
        {   "description": "t = volume up", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "t" },

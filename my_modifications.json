        {   "description": "f = next tab", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "f" },		    
            "to": [ { "key_code": "tab", "modifiers": [ "left_control" ] } ] },
        {   "description": "g = volume down", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "g" },
            "to": [ { "consumer_key_code": "volume_decrement" } ] },
        {   "description": "x = x", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "x" },
            "to": [ { "key_code": "x" } ] },
        {   "description": "c = prev outlook pane", "type": "basic", "conditions": [ { "type" : "variable_if", "name": "layer_name", "value": "nav" } ],
            "from": { "key_code": "c" },
            "to": [ { "key_code": "open_bracket", "modifiers": [ "left_control", "left_shift" ] } ] },
